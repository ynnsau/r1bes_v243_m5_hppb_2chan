--synthesis_resources = reg 20 
OPTIONS ALTERA_INTERNAL_OPTION = "AUTO_SHIFT_REGISTER_RECOGNITION=OFF";

SUBDESIGN dffpipe_ppsr1
( 
	clock	:	input;
	clrn	:	input;
	d[9..0]	:	input;
	q[9..0]	:	output;
) 
VARIABLE 
	dffe3a[9..0] : dffe;
	dffe4a[9..0] : dffe;
	ena	: NODE;
	prn	: NODE;
	sclr	: NODE;

BEGIN 
	dffe3a[].clk = clock;
	dffe3a[].clrn = clrn;
	dffe3a[].d = (d[] & (! sclr));
	dffe3a[].ena = ena;
	dffe3a[].prn = prn;
	dffe4a[].clk = clock;
	dffe4a[].clrn = clrn;
	dffe4a[].d = (dffe3a[].q & (! sclr));
	dffe4a[].ena = ena;
	dffe4a[].prn = prn;
	ena = VCC;
	prn = VCC;
	q[] = dffe4a[].q;
	sclr = GND;
END;
--VALID FILE
